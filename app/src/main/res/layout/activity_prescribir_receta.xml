<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="rsm.com.ar.rsmapp.activities.PrescribirRecetaActivity">

    <TextView
        android:id="@+id/prescribir_paciente"
        android:layout_width="352dp"
        android:layout_height="47dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" />

    <EditText
        android:id="@+id/prescribir_nombre"
        android:layout_width="352dp"
        android:layout_height="51dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/prescribir_paciente"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:hint="@string/nombre_medicamento"
        android:textSize="20sp"/>

    <EditText
        android:id="@+id/prescribir_diagnostico"
        android:layout_width="351dp"
        android:layout_height="44dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/prescribir_nombre"
        android:hint="@string/diagnostico"/>
    <!--





-->
</android.support.constraint.ConstraintLayout>
